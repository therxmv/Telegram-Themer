    <style name="Theme.TelegramThemer" parent="Theme.Material3.Dark.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/primary_200</item>
        <item name="colorPrimaryVariant">@color/primary_800</item>
        <item name="colorOnPrimary">@color/black</item>
